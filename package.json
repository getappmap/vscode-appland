{
	"publisher": "appland",
	"name": "appmap",
	"displayName": "AppMap",
	"description": "Show AppMap diagram for context object",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:appland.scenarioFile"
	],
	"license": "MIT",
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "appland.scenarioFile",
				"displayName": "AppLand Scenario",
				"selector": [
					{
						"filenamePattern": "*.appmap.json"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"lint": "eslint . --ext .ts",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js && node ./tape/**-test.js",
		"tape": "node ./tape/**-test.js",
		"compile": "rollup -c rollup.config.js -m",
		"watch": "rollup -c rollup.config.js --watch -m"
	},
	"devDependencies": {
		"@rollup/plugin-eslint": "^8.0.1",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-sucrase": "^3.1.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"eslint": "^7.15.0",
		"eslint-plugin-import": "^2.22.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"node-sass": "^5.0.0",
		"rollup": "^2.35.1",
		"rollup-plugin-scss": "^2.6.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-pluginutils": "^2.8.2",
		"sucrase": "^3.16.0",
		"tape": "^5.0.1",
		"tslib": "^2.0.3",
		"typescript": "^4.1.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@appland/diagrams": "github:applandinc/diagrams-js#master",
		"@appland/models": "github:applandinc/models-js#master",
		"bootstrap": "^4.5.3",
		"bootstrap-autocomplete": "^2.3.7",
		"d3": "^5.16.0",
		"jquery": "^3.5.1",
		"popper": "^1.0.1"
	}
}
